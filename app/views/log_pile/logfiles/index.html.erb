
<table class="bordered-table">
	<thead>
		<tr>
			<th>Path</th>
			<th>Last Modified</th>
		</tr>		
	</thead>
	<tbody>
		<% @files.each do |file| %>
		  <tr>
		  	<td>
		  		<%= link_to file.name, logfile_path(file) %>
		  	</td>
		  	<td>
					<%= file.file_last_modified %>
		  	</td>
		  	<td>
		  		Edit
		  	</td>
		  </tr>	
		<% end %>		
	</tbody>
</table>